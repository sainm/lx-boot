<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lx.boot.psy.mapper.ScaleMapper">

    <!-- 获取量主分页列表 -->
    <select id="getScalePage" resultType="com.lx.boot.psy.model.vo.ScaleVO">
        SELECT
                id,
                name,
                code,
                description,
                applicable_age,
                applicable_gender,
                create_by,
                create_time,
                update_by,
                update_time,
                is_deleted
        FROM
            psy_scale
        <where>
        </where>
    </select>

</mapper>
