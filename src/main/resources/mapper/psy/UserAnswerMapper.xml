<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lx.boot.psy.mapper.UserAnswerMapper">

    <!-- 获取用户答题记录分页列表 -->
    <select id="getUserAnswerPage" resultType="com.lx.boot.psy.model.vo.UserAnswerVO">
        SELECT
                id,
                record_id,
                question_id,
                option_id,
                answer_text,
                score,
                time_spent,
                create_by,
                update_by,
                create_time,
                update_time
        FROM
            psy_user_answer
        <where>
        </where>
    </select>

</mapper>
