<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lx.boot.psy.mapper.NormSampleMapper">

    <!-- 获取量常模样本定义分页列表 -->
    <select id="getNormSamplePage" resultType="com.lx.boot.psy.model.vo.NormSampleVO">
        SELECT
                id,
                version_id,
                sample_name,
                gender,
                age_min,
                age_max,
                region,
                sample_size,
                description,
                create_by,
                update_by,
                create_time,
                update_time
        FROM
            psy_norm_sample
        <where>
        </where>
    </select>

</mapper>
