<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lx.boot.psy.mapper.QuestionWeightMapper">

    <!-- 获取计分规则题目权重分页列表 -->
    <select id="getQuestionWeightPage" resultType="com.lx.boot.psy.model.vo.QuestionWeightVO">
        SELECT
                id,
                rule_id,
                question_id,
                weight,
                create_by,
                create_time,
                update_by,
                update_time,
                is_deleted
        FROM
            psy_question_weight
        <where>
        </where>
    </select>

</mapper>
